{
  "manifest_version": 2,
  "name": "Steadfast Extension",
  "version": "1.0",
  "description": "Streak Algo Automation",
  "permissions": ["activeTab", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
